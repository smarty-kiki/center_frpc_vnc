#!/bin/bash

ROOT_DIR="$(cd "$(dirname $0)" && pwd)"/..
HOSTNAME=`hostname`

VNC_CENTER_PORT=$

if [ -z "$VNC_CENTER_PORT" ]
then
    echo "用法 $0 端口"
    exit
fi

HARDWARE=`uname -m`

case $HARDWARE in
    x86_64)
        BIN_NAME=frpc_386
        ;;
    armv7l)
        BIN_NAME=frpc_arm
        ;;
    armv6l)
        BIN_NAME=frpc_arm
        ;;
    aarch64)
        BIN_NAME=frpc_arm
        ;;
    *)
        echo "未识别的硬件架构"
esac

HOSTNAME=$HOSTNAME VNC_CENTER_PORT=$VNC_CENTER_PORT $ROOT_DIR/bin/hardware.d/$BIN_NAME -c $ROOT_DIR/config/frpc.ini
